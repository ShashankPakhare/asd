<%- include('header')%>

    
   <div class="container">
    <h1><%= title %></h1>
    <p>Welcome to <%= title %></p>
    
    <table class="table">
      <thead class="thead-dark">
        <tr>
          
          <th scope="col">name</th>
          <th scope="col">email</th>
          <th scope="col">etype</th>
          <th scope="col">hourlyrate</th>
          <th scope="col">totalhour</th>
        </tr>
      </thead>
      
     <% records.forEach(function(row){%>
      
      <tr>
        <td><%= row.name%></td>
        <td><%= row.email%></td>
        <td><%= row.etype%></td>
        <td><%= row.hourlyrate%></td>
        <td><%= row.totalhour%></td>
      </tr>
      
     <%})%>
      
    </table>
    
    <div class='container'>
    <form action="/" class="form-horizontal" method="post">
      <div class="form-group">
        <label for="Name">name</label>
        <input type="text" class="form-control" id="Name" aria-describedby="emailHelp" placeholder="Name" name="name">
         </div>

      <div class="form-group">
        <label for="em">email</label>
        <input type="email" class="form-control" id="em" placeholder="email" name="mail">
      </div>


      <div class="form-group">
        <label for="etype">etype</label>
        <input type="text" class="form-control" id="etype" aria-describedby="emailHelp" placeholder="etype" name="etype">
        </div>

      <div class="form-group">
        <label for="hrate">hourlyrate</label>
        <input type="text" class="form-control" id="hrate" aria-describedby="emailHelp" placeholder="hourly rate" name="hrate">
      </div>

      <div class="form-group">
        <label for="Name">totalhour</label>
        <input type="text" class="form-control" id="totalhour" aria-describedby="emailHelp" placeholder="total hour" name="totalhour">
      </div>
     
      <button type="submit" class="btn btn-primary">Submit</button>
    </form>

  </div>
   </div>



  
   <%- include('footer')%>
